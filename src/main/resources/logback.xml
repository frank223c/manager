<configuration>
    <appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d %p [%c] - %m%n</pattern>
        </encoder>
    </appender>
    <logger name="com.suny.association.mapper" level="debug"/>
    <logger name="org.mybatis" level="debug"/>
    <root level="info">
        <appender-ref ref="stdout"/>
    </root>
</configuration>